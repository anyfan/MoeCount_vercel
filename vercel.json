{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "content-type"
                },
                {
                    "key": "content-type",
                    "value": "image/svg+xml"
                },
                {
                    "key": "cache-control",
                    "value": "max-age=0, no-cache, no-store, must-revalidate"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/index"
        }
    ]
}